- name: epy_block_0
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nfrom gnuradio import gr\n\nclass blk(gr.sync_block):\
      \  # other base classes are basic_block, decim_block, interp_block\n    \"\"\
      \"Embedded Python Block example - a simple multiply const\"\"\"\n\n    def __init__(self,\
      \ hpa=1):  # only default arguments here\n        \"\"\"arguments to this function\
      \ show up as parameters in GRC\"\"\"\n        gr.sync_block.__init__(\n    \
      \        self,\n            name='Amplifiers',   # will show up in GRC\n   \
      \         in_sig=[np.complex64],\n            out_sig=[np.complex64]\n     \
      \   )\n        self.hpa = np.array(hpa)\n\n    def amplifier(self, signal_in):\n\
      \        ibo_range = self.hpa[:, 0]\n        obo_range = self.hpa[:, 1]\n  \
      \      phase_range = self.hpa[:, 2]\n        signal_in_am = 20 * np.log10(np.abs(signal_in))\n\
      \        signal_in_pm = np.angle(signal_in)\n        signal_out_am = np.interp(signal_in_am,\
      \ ibo_range, obo_range)\n        signal_out_pm = np.interp(signal_in_am, ibo_range,\
      \ phase_range) * np.pi / 180\n        signal_out = 10**(signal_out_am / 20)\
      \ * np.exp(1j * (signal_in_pm + signal_out_pm))\n        \n        return signal_out\n\
      \    \n    def work(self, input_items, output_items):\n        signal_in = input_items[0]\n\
      \        signal_out = self.amplifier(signal_in)\n\n        output_items[0][:]\
      \ = signal_out\n\n        return len(output_items[0])\n"
    affinity: ''
    alias: ''
    comment: ''
    hpa: hpa
    maxoutbuf: '0'
    minoutbuf: '0'
  states:
    _io_cache: ('Amplifiers', 'blk', [('hpa', '1')], [('0', 'complex', 1)], [('0',
      'complex', 1)], 'Embedded Python Block example - a simple multiply const', ['hpa'])
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [512, 400.0]
    rotation: 0
    state: enabled
